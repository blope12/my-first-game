[sub_resource type="GDScript" id="GDScript_iqe28"]
script/source = "extends Camera2D

@export var strength := 10
@export var speed := 1

var time := 0.0
var base_offset := Vector2.ZERO

func _ready():
	base_offset = offset

func _process(delta):
	time += delta * speed

	var shake_x = sin(time * 1.5) * strength
	var shake_y = cos(time * 1.2) * strength * 0.5

	# گرد کردن برای پیکسل perfect
	offset = (base_offset + Vector2(shake_x, shake_y)).round()
"

