var player_in_area = false
@onready var label = $closetlable
var dialogue_resource = preload(\"res://scene/room1/closet.dialogue\")

func _ready():
	label.visible = false

func _unhandled_input(event: InputEvent) -> void:


func _on_body_entered(body):
	if body.name == \"player\":
		player_in_area = true
		label.text = \"PRESS E\"
		label.visible = true

func _on_body_exited(body):
	if body.name == \"player\":
		player_in_area = false
		label.visible = false
"

