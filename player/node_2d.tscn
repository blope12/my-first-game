resource_name = "player"
script/source = "extends CharacterBody2D

const SPEED = 70
const GRAVITY = 800

@onready var sprite_2d: Sprite2D = $Sprite2D
@onready var sprite = $idle
@onready var anim = $AnimationPlayer
@onready var cam := $Camera2D  

# Shake variables
@export var shake_strength := 5
@export var shake_speed := 0.3

func _ready():
	add_to_group(\"Player\")  # حالا همه جا پیدا می‌شه!
func _physics_process(delta):
	# Optional: Press \"S\" to save manually for testing
	if Input.is_key_pressed(KEY_S):
		if get_node_or_null(\"/root/GameSave\"):
			get_node(\"/root/GameSave\").save_game(self)

	global_position = global_position.round()
	# ... (rest of your movement code)
	if not is_on_floor():
		velocity.y += GRAVITY * delta
	else:
		velocity.y = 0

	var dir = 0
	if Input.is_key_pressed(KEY_A):
		dir -= 1
	if Input.is_key_pressed(KEY_D):
		dir += 1

	velocity.x = dir * SPEED

	if dir != 0:
		anim.play(\"walk\")
		sprite.flip_h = dir < 0
	else:
		anim.play(\"idle\")

	move_and_slide()
"

